{
  "name": "@local/github-actions",
  "version": "0.0.0",
  "private": true,
  "description": "GitHub Actions core library",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./lib tsconfig.build.tsbuildinfo",
    "build": "tsc --build tsconfig.build.json",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint **/*.ts",
    "test": "jest"
  },
  "dependencies": {
    "@actions/core": "1.10.1",
    "@actions/github": "6.0.0",
    "ts-log": "2.2.5",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@types/uuid": "9.0.8"
  }
}
